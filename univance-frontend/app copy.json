{
  "expo": {
    "name": "univance-frontend",
    "slug": "univance-frontend",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png",
      "bundler": "metro"
    },
    "scheme": "univance",
    
    "assetBundlePatterns": [
      "**/*"
    ],
    
    "packagerOpts": {
      "assetExts": ["ttf", "otf"]
    },
    
    "fonts": [
      {
        "asset": "./src/assets/fonts/Nunito-Regular.ttf",
        "fontFamily": "Nunito-Regular"
      },
      {
        "asset": "./src/assets/fonts/Nunito-Bold.ttf",
        "fontFamily": "Nunito-Bold" 
      },
      {
        "asset": "./src/assets/fonts/Nunito-SemiBold.ttf",
        "fontFamily": "Nunito-SemiBold"
      },
      {
        "asset": "./src/assets/fonts/Nunito-Black.ttf",
        "fontFamily": "Nunito-Black"
      },
      {
        "asset": "./src/assets/fonts/Nunito-BlackItalic.ttf", 
        "fontFamily": "Nunito-BlackItalic"
      },
      {
        "asset": "./src/assets/fonts/Nunito-BoldItalic.ttf",
        "fontFamily": "Nunito-BoldItalic"
      },
      {
        "asset": "./src/assets/fonts/Nunito-ExtraBold.ttf",
        "fontFamily": "Nunito-ExtraBold"
      },
      {
        "asset": "./src/assets/fonts/Nunito-ExtraBoldItalic.ttf",
        "fontFamily": "Nunito-ExtraBoldItalic"
      },
      {
        "asset": "./src/assets/fonts/Nunito-ExtraLight.ttf",
        "fontFamily": "Nunito-ExtraLight"
      },
      {
        "asset": "./src/assets/fonts/Quicksand-Regular.ttf",
        "fontFamily": "Quicksand-Regular"
      },
      {
        "asset": "./src/assets/fonts/Quicksand-Bold.ttf",
        "fontFamily": "Quicksand-Bold"
      },
      {
        "asset": "./src/assets/fonts/Quicksand-Light.ttf",
        "fontFamily": "Quicksand-Light" 
      },
      {
        "asset": "./src/assets/fonts/Quicksand-Medium.ttf",
        "fontFamily": "Quicksand-Medium"
      },
      {
        "asset": "./src/assets/fonts/Quicksand-SemiBold.ttf",
        "fontFamily": "Quicksand-SemiBold"
      }
    ]
  }
}